public class printnum {
    public static void main (String[] args){
		int num=0;
    	        for(int i = 1;i<=20;i++){
					num+=calNum(i);
				}
				System.out.println(num);
					
				}
				public static int calNum (int n){
					if(n<0){System.out.println("数据错误");return 0;}
					if(n==0){return 1 ;}
					return calNum(n-1)*n;
				}
    }  
